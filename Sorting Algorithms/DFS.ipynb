    "def dfs(node,adj,visited):\n",
    "    visited[node] = True\n",
    "    print(node,end = \" \")\n",
    "    for neighbor in adj[node]:\n",
    "        if not visited[neighbor]:\n",
    "            dfs(neighbor,adj,visited)"
   ]
